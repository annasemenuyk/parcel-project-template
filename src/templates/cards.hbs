{{#each this}}
    <div class="photo-card">
        <a class="gallery__item" href="{{largeImageURL}}">
            <div class="info">
                <img class="gallery__image" src="{{webformatURL}}" alt="{{tags}}" loading="lazy" />
            
                <div class="info-list">
                    <p class="info-item">
                        <span class="info-span"><b>Likes</b>
                        {{likes}}</span>
                    </p>
                    <p class="info-item">
                        <b>Views</b>
                        {{views}}
                    </p>
                    <p class="info-item">
                        <b>Comments</b>
                        {{comments}}
                    </p>
                    <p class="info-item">
                        <b>Downloads</b>
                        {{downloads}}
                    </p>
                </div>
            </div>
        </a>
    </div>
{{/each}} 